<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">My Characters</h3>
    </div>
  <div class="panel-body">

    <table class='table table-hover table-striped'>
      <h3>You just entered a new character:</h3>
      <p>First name: <%= @newfirstname %></p>
      <p>Last name: <%= @newlastname %></p>
      <p>Birthday: <%= @newage %></p>
      <p>Origin: <%= @neworigin %></p>
    </table>

  </div>
  </div>
</div>
